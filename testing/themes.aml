}

PageTheme sakura {
  custom_css: @css
      @import url('https://fonts.googleapis.com/css2?family=Playwrite+GB+S:ital,wght@0,100..400;1,100..400&display=swap');
  ;;
  background {
      bg_image: 'linear-gradient(to top, #FFF5FB, #FAE0EA)'
    }
    canvas: CanvasTheme {
      border {
        border_width: 2
        border_color: "#F6C9E4"
        border_radius: 12
      }
      shadow: 'none'
    }
  
  viz: VizTheme {
    table {
      general {
        bg_color: '#FAE0EA';
        font_family: 'Playwrite GB S'
        banding_color: 'white'
        hover_color: '#F5CEDD'; 
        font_size: '12px'; 
        font_color: '#852C50'; 
        border_color: '#DA7AA1'; 
        grid_color: '#DA7AA1'; 
      }

      // Header Styles
      header {
        bg_color: '#F8A4C5';
        font_size: '14px'; 
        font_weight: 'bold'; 
      }

      // Row Header Styles
      row_header {
        bg_color: '#F8A4C5';
        font_size: '12px'; 
        font_weight: 'bold'; 
      }

      // Column Header Styles
      column_header {
        bg_color: 'linear-gradient(to top, #FFF0F6, white)';
        font_size: '12px'; 
        font_weight: 'normal'; 
      }

      // Subtitle Styles
      sub_title {
        font_size: '12px';
        font_weight: 'light';
      }
    }
  }
}



PageTheme classic_blue {
  background {
    bg_color: "#FFFFFF"
    bg_repeat: false
    bg_size: "cover"
    // bg_image:
  }
  canvas {
    border {
      border_radius: 4
      border_width: 1
      border_color: "#4896EA"
      border_style: "solid"
    }
    background {
      bg_color: "#D1E5FA"
      bg_repeat: false
      bg_size: "cover"
      // bg_image:
    }
    shadow: "none"
    opacity: 1
  }
  block {
    label {
      font_family: "Inter"
      font_size: 14
      font_color: "#1357A0"
      font_weight: "medium"
      font_style: "normal"
    }
    text {
      font_family: "Inter"
      font_size: 12
      font_color: "#1357A0"
      font_weight: "normal"
      font_style: "normal"
    }
    border {
      border_width: 1
      border_radius: 8
      border_color: "#4896EA"
      border_style: "solid"
    }
    background {
      bg_color: "#E8F2FD"
      bg_repeat: false
      bg_size: "cover"
      // bg_image:
    }
    padding: 12
    shadow: "none"
    opacity: 1
  }

  //Viz Theme
  viz {
    // Table Theme
    table {
      general {
        bg_color: 'white'
        hover_color: '#C8DCF2'
        banding_color: '#EAF5FC'
        font_size: 12
        font_color: '#505D6A'
        font_family: 'Arial'
        border_color: '#90A2B7'
        border_width: 1
        grid_color: '#90A2B7'
      }
      header {
        bg_color: '#AECEF2'
        font_size: 12
        font_color: '#505D6A'
        font_weight: 'bold'
      }
      
      // Pivot table attributes
      row_header {
        bg_color: 'white'
        font_size: 12
        font_color: '#505D6A'
        font_weight: 'bold'
      }
      column_header {
        bg_color: '#608EC3'
        font_size: 12
        font_color: 'white'
        font_weight: 'bold'
      }

      // Metric Sheet attributes
      sub_title {
        font_size: 12
        font_color: '#505D6A'
        font_weight: 'bold'
      }
    }
  }
}


