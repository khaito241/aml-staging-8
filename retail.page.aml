          }
        },
        VizFieldFull {
          ref: ref('retail_promotion', 'geography')
          format {
            type: 'number'
            pattern: 'inherited'
          }
        }
      ]
      settings {
        show_row_number: true
        aggregate_awareness {
          enabled: true
          debug_comments: true
