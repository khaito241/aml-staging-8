Dashboard untitled_55 {
  title: 'Retail'
  description: ''''''

  view: CanvasLayout {
    label: 'View 1'
    height: 840
    grid_size: 20
    block v1 {
      position: pos(20, 20, 400, 300)
      layer: 1
    }
    block v2 {
      position: pos(440, 20, 400, 300)
      layer: 2
    }
    block v3 {
      position: pos(20, 340, 400, 300)
      layer: 3
    }
    block v4 {
      position: pos(440, 340, 400, 300)
      layer: 4
    }
    block v5 {
      position: pos(860, 20, 400, 300)
      layer: 5
    }
  }

  theme: H.themes.classic
  block v1: VizBlock {
    label: 'Promotion'
    viz: DataTable {
      dataset: retail
      fields: [
        VizFieldFull {
          ref: ref('retail_promotion', 'upc')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('retail_promotion', 'store')
          format {
            type: 'number'
            pattern: 'inherited'
          }
